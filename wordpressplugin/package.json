{
  "name": "n8n-chat",
  "version": "1.0.0",
  "description": "Beautiful chat widget for n8n workflows in WordPress",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm:dev:*\"",
    "dev:frontend": "vite --mode development",
    "dev:admin": "wp-scripts start src/admin-index.tsx --output-path=build/admin",
    "build": "npm run build:frontend && npm run build:admin",
    "build:frontend": "vite build",
    "build:admin": "wp-scripts build src/admin-index.tsx --output-path=build/admin",
    "lint": "eslint src/ --ext .ts,.tsx",
    "lint:fix": "eslint src/ --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "plugin-zip": "wp-scripts plugin-zip",
    "clean": "rimraf build"
  },
  "dependencies": {
    "@assistant-ui/react": "^0.11.53",
    "@assistant-ui/styles": "^0.3.2",
    "autoprefixer": "^10.4.23",
    "postcss": "^8.5.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^4.1.18"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@tailwindcss/vite": "^4.1.18",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.2.0",
    "@wordpress/scripts": "^27.0.0",
    "concurrently": "^8.2.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "rimraf": "^5.0.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "browserslist": [
    "extends @wordpress/browserslist-config"
  ]
}
